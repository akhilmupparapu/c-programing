#include <stdio.h>

int main() {
    int arr[] = {12, 56, 34, 78, 100};
    int n = sizeof(arr) / sizeof(arr);
    int max = arr;
    for (int i = 1; i < n; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    printf("The largest element in the array is %d\n", max);
    return 0;
}
